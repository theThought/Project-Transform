' ********************************************************************************
Sub SetProperty(Properties, Name, Value)
   Dim Property
   Set Property = Properties.FindItem(Name)
   If IsNullObject(Property) Then
       Set Property = Properties.CreateProperty()
       Property.Name = Name
       Property.Value = Value
       Properties.Add(Property)
   Else
       Property.Value = Value
   End If
End Sub

